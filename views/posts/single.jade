extends layouts\default
block content
    if post:
        article(id='post-'+get_the_ID(), class=get_post_classes(), pubdate)
            header.entry-header
                h2.entry-title
                    a(href=get_the_permalink(), rel='bookmark')
                        span= get_the_title()
                .meta
                  !=get_post_entry_date(false)
            #post_content
                != get_the_content()
    else
      p No post was found here!